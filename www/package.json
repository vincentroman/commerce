    "@angular/platform-browser": "5.2.6",
    "@angular/platform-browser-dynamic": "5.2.6",
    "@angular/router": "5.2.6",
    "@angular/upgrade": "5.2.6",
    "@types/typeahead": "0.11.32",
    "angular-in-memory-web-api": "0.5.3",
    "bootstrap": "3.3.7",
    "core-js": "2.5.3",
