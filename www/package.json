{
  "name": "weweave-commerce-www",
  "version": "0.0.1",
  "scripts": {
    "serve": "lite-server -c=bs-config.json",
    "start": "npm run css && concurrently \"npm run build:watch\" \"npm run serve\"",
    "build:watch": "tsc -p src/ -w",
    "css": "grunt css",
    "clean": "rm -rf dist/",
    "grunt": "grunt",
    "webpack": "webpack --config webpack.prod.js --progress --profile --bail",
    "build": "npm run grunt && npm run webpack && cp src/index.prod.html dist/index.html"
