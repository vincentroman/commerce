{
  "name": "weweave-commerce-www",
  "version": "0.0.1",
  "scripts": {
    "serve": "lite-server -c=bs-config.json",
    "start": "npm run css && concurrently \"npm run build:watch\" \"npm run serve\"",
    "build:watch": "tsc -p src/ -w",
    "css": "grunt css",
    "clean": "rm -rf dist/",
    "grunt": "grunt",
    "webpack": "webpack --config webpack.prod.js --progress --profile --bail",
    "build": "npm run grunt && npm run webpack && cp src/index.prod.html dist/index.html"
  },
  "license": "GPL-3.0",
  "dependencies": {
    "@angular/common": "4.3.5",
    "@angular/compiler": "4.3.5",
    "@angular/core": "4.3.5",
    "@angular/forms": "4.3.5",
    "@angular/http": "4.3.5",
    "@angular/platform-browser": "4.3.5",
    "@angular/platform-browser-dynamic": "4.3.5",
    "@angular/router": "4.3.5",
    "@angular/upgrade": "4.3.5",
    "@types/typeahead": "0.11.30",
    "angular-in-memory-web-api": "0.3.2",
    "bootstrap": "3.3.7",
    "core-js": "2.5.0",
    "jquery": "3.2.1",
    "moment": "2.18.1",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.4.3",
    "systemjs": "0.20.17",
    "typeahead.js": "0.11.1",
    "zone.js": "0.8.16"
  },
  "devDependencies": {
    "@types/node": "8.0.24",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.2.3",
    "babili-webpack-plugin": "0.1.2",
    "concurrently": "3.5.0",
    "grunt": "1.0.1",
    "grunt-bootlint": "0.10.2",
    "grunt-cli": "1.2.0",
    "grunt-sass": "2.0.0",
    "grunt-ts": "6.0.0-beta.16",
    "grunt-tslint": "5.0.1",
    "html-loader": "0.5.1",
    "lite-server": "2.3.0",
    "tslint": "5.6.0",
    "typescript": "2.4.2",
    "webpack": "3.5.5"
  }
